{:paths ["notes"]
 :deps {io.github.nextjournal/clerk {:mvn/version "0.17.1102"}
        org.clojure/core.logic {:mvn/version "1.1.0"}}
 :aliases
 {:dev
  {:exec-fn nextjournal.clerk/serve!
   :exec-args {:browse true :watch-paths ["notes"]}}
  :nextjournal/clerk
  {:exec-fn nextjournal.clerk/build!
   :exec-args {:paths [
                       "index.clj"
                       "notes/*.clj"]
               :out-path "docs"
               :package :directory
               :verbose? true}}}}
